class ArrayClass {
    public int calc(int[] x, int[] y) {
        return y.length * y[1] / x[0];
    }

    public static void main(String[] args) {
        int[] x;
        int[] y;

        x = new int[1];
        y = new int[x.length + 1];

        x[0] = 4;
        y[0] = 0 - 3 + x[0];
        y[y[0]] = (x[0] + y[0]) * 2;

        io.println(new ArrayClass().calc(x, y));
    }
}